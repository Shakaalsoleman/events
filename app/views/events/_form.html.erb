<%= form_with model: @event, local: true do |form| %>

    <% if @event.errors.any? %>
        <div id="error_explanation">
          <h2>
            <%= pluralize(@event.errors.count, "error") %> prohibited
            this event from being saved:
          </h2>
          <ul>
            <% @event.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
            <% end %>
          </ul>
        </div>
    <% end %>
    <p>
      <%= form.label :Id %><br>
      <%= form.number_field :Id %>
    </p>

    <p>
      <%= form.label :name %><br>
      <%= form.text_field :name %>
    </p>


    <p>
      <%= form.label :longitude %><br>
      <%= form.number_field :longitude %>
    </p>

    <p>
      <%= form.label :latitude %><br>
      <%= form.number_field :latitude %>
    </p>

    <p>
      <%= form.submit %>
    </p>
<% end %>